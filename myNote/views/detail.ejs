<%- include header.ejs%>
        <div class="container center-block">
            <div class="row">
                <div class="col-xs-8 col-xs-offset-2">
                    <div class="article-box">
                        <h1 class="text-center"><%= note.title%></h1>
                        <p class="text-center">
                            <span class="label label-info"><%= note.tag %>&nbsp;</span> |
                            <span class="create-time">
                                <span class="text-bold">时间：</span>
                                <%= moment(note.createTime).locale('zh-cn').format('LL HH:mm:ss')%>
                            </span>
                        </p>
                        <p><%= note.content%></p>
                    </div>
                </div>

            </div>

        </div>
<%- include footer.ejs%>